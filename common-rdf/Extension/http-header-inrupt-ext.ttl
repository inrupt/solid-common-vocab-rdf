prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#>
prefix xsd:      <http://www.w3.org/2001/XMLSchema#>
prefix owl:      <http://www.w3.org/2002/07/owl#>
prefix dcterms:  <http://purl.org/dc/terms/>
prefix skos:     <http://www.w3.org/2004/02/skos/core#>
prefix ldp:      <http://www.w3.org/ns/ldp#>
prefix schema:   <http://schema.org/>
prefix vann:     <http://purl.org/vocab/vann/>
prefix dalicc:   <http://dalicc.net/licenselibrary/>


prefix http: <http://www.w3.org/2007/ont/http#>

# This vocab never provided an explicit vocab IRI (i.e., it defines it as a
# relative IRI using `<> a owl:Ontology`), which was a bad idea, as it would
# have originally been a `http://...` IRI, but subsequent automatic redirects
# from `http:` to `https:` causes RDF parsers to now treat it as `https:`.
# So for consistency going forward, we need to explicitly use the `https:`
# scheme (which will break anyone using the original IRIs!).
prefix httph: <https://www.w3.org/2007/ont/httph#>

prefix inrupt_gen: <https://inrupt.com/vocab/tool/artifact_generator/>
prefix httph_inrupt_ext: <https://inrupt.com/vocab/extension/httph#>

#
# Extensions deliberately add to (i.e., extend) existing terms in existing
# vocabularies, specifically to add extra features, such as multilingual
# values for labels and comments.
#
# So we very deliberately re-use the underlying vocab's namespace, but use our
# own prefix to try and differentiate.
#
httph_inrupt_ext: a inrupt_gen:OntologyExtension ;
    owl:versionInfo "0.0.1" ;
    owl:versionIRI <https://inrupt.com/vocab/extension/httph/0.0.1> ;
    dcterms:creator <https://inrupt.com/profile/card/#us> ;
    dcterms:issued "2019/07/28"^^xsd:date ;
    dcterms:title "Inrupt extension adding terms and multilingual meta-data for HTTP headers" ;
    dcterms:description "Inrupt extension to the HTTPH (HTTP Headers) vocabulary providing terms for more headers than the current (Aug 2019) vocab" ;
    vann:preferredNamespacePrefix "httph_inrupt_ext" ;
    vann:preferredNamespaceUri "https://www.w3.org/2007/ont/httph#" ;
    dcterms:license dalicc:MIT .

httph:accept a rdf:Property ;
    rdfs:label "Accept"@en ;
    rdfs:comment "See the HTTP specification"@en ;
    rdfs:domain http:Message ;
    rdfs:isDefinedBy httph: ;
    rdfs:range rdfs:Literal .

httph:accept-language a rdf:Property ;
    rdfs:label "Accept-Language"@en ;
    rdfs:comment "See the HTTP specification"@en ;
    rdfs:domain http:Message ;
    rdfs:isDefinedBy httph: ;
    rdfs:range rdfs:Literal .
