prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#>
prefix xsd:      <http://www.w3.org/2001/XMLSchema#>
prefix owl:      <http://www.w3.org/2002/07/owl#>
prefix dcterms:  <http://purl.org/dc/terms/>
prefix skos:     <http://www.w3.org/2004/02/skos/core#>
prefix ldp:      <http://www.w3.org/ns/ldp#>
prefix schema:   <http://schema.org/>
prefix vann:     <http://purl.org/vocab/vann/>
prefix solid-terms: <http://www.w3.org/ns/solid/terms#>
prefix solid-terms-inrupt-ext: <https://w3id.org/inrupt/vocab/extension/solid-terms#>

#
# Extensions deliberately add (i.e. extend) existing terms in existing
# vocabularies, specifically to add extra features, such as multi-lingual
# values for labels and comments.
#
# So we very deliberately re-use the underlying vocab's namespace, but use our
# own prefix to try and differentiate.
#
solid-terms-inrupt-ext:
    a owl:Ontology;
    owl:versionInfo "0.1" ;
    dcterms:creator <https://inrupt.com/profile/card/#us> ;
    dcterms:issued "2020/10/02"^^xsd:date ;
    dcterms:title "Inrupt extension adding new Solid Terms" ;
    dcterms:description "Extension to the Solid Terms vocabulary adding new terms" ;
    vann:preferredNamespacePrefix "solid-terms-inrupt-ext" ;
    vann:preferredNamespaceUri "http://www.w3.org/ns/solid/terms#" ;
    dcterms:license "http://www.opendatacommons.org/licenses/pddl/" .


solid-terms:WebId a rdfs:Class ;
    rdfs:label "Class of WebID"@en ;
    rdfs:comment "The class of WebIDs - where a WebID is an HTTP URI which refers to an Agent (Person, Organization, Group, Device, etc.)."@en ;
    rdfs:isDefinedBy solid-terms-inrupt-ext: .

solid-terms:webId a rdf:Property ;
    rdfs:label "WebID"@en ;
    rdfs:comment "A WebID is an HTTP URI which refers to an Agent (Person, Organization, Group, Device, etc.)."@en ;
    rdfs:isDefinedBy solid-terms-inrupt-ext: .

solid-terms:podOwner a rdf:Property ;
    rdfs:label "Pod Owner"@en ;
    rdfs:comment "The WebID of a 'Pod Owner', the person, organisation or agent deemed to be the owner of a Solid Pod."@en ;
    rdfs:isDefinedBy solid-terms-inrupt-ext: ;
    schema:rangeIncludes solid-terms:WebID .
